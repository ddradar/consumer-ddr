{
  "name": "consumer-ddr",
  "version": "0.6.1",
  "description": "Consumer DDR Song List",
  "author": "ddradar",
  "private": true,
  "engines": {
    "node": ">=16",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate",
    "fix": "run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{js,ts,vue,json,yaml,yml,scss}\"",
    "lint:tsc": "nuxt typecheck",
    "pretest": "nuxt prepare",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddradar/consumer-ddr.git"
  },
  "dependencies": {
    "@oruga-ui/oruga-next": "^0.5.5",
    "@oruga-ui/theme-bulma": "^0.2.6"
  },
  "devDependencies": {
    "@nuxt/content": "^2.0.1",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.2",
    "ajv": "^8.11.0",
    "c8": "^7.12.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^20.0.0",
    "npm-run-all2": "^5.0.2",
    "nuxt": "^3.0.0-rc.7",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "typescript": "^4.7.4",
    "vitest": "^0.21.1"
  },
  "resolutions": {
    "@types/node": "16.x"
  }
}
