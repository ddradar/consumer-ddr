{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["@nuxt/eslint-config", "plugin:nuxt/recommended", "prettier"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "no-process-env": "error",
    "sort-imports": "off",
    "import/order": "off",
    "simple-import-sort/imports": "error",
    "vue/multi-word-component-names": "off"
  }
}
